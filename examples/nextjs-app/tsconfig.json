{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": ["kakao.maps.d.ts"],
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "emitDeclarationOnly": false,
    "noEmit": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
